{
  "name": "api",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register server.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "db:create-products": "ts-node -r tsconfig-paths/register scripts/create-products.ts",
    "db:migrate": "knex migrate:latest --knexfile config/knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile config/knexfile.ts",
    "db:create": "knex migrate:make --knexfile config/knexfile.ts",
    "db:down": "knex migrate:down --knexfile config/knexfile.ts",
    "db:seed": "knex seed:run --knexfile config/knexfile.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check \"**/*.{js,ts,tsx,jsx,json,css,scss,md}\"",
    "format:fix": "prettier --write \"**/*.{js,ts,tsx,jsx,json,css,scss,md}\""
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express4": "^1.1.2",
    "@aws-sdk/client-s3": "^3.848.0",
    "@getbrevo/brevo": "^2.0.0-beta.4",
    "@sentry/node": "^5.30.0",
    "@types/cors": "^2.8.19",
    "@types/jspdf": "^2.0.0",
    "@types/linkify-it": "^5.0.0",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "bunyan": "^1.8.15",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "fastest-validator": "^1.19.1",
    "graphql": "^16.11.0",
    "graphql-rate-limit-directive": "^2.0.6",
    "graphql-resolvers": "^0.4.2",
    "graphql-tag": "^2.12.6",
    "handlebars": "^4.7.8",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "knex": "^3.1.0",
    "linkify-it": "^5.0.0",
    "lodash": "^4.17.21",
    "mjml": "^4.15.3",
    "morgan": "^1.10.1",
    "mysql2": "^3.14.2",
    "node-cron": "^3.0.3",
    "openai": "^4.27.0",
    "p-limit": "^3.1.0",
    "rate-limiter-flexible": "^7.1.1",
    "sharp": "^0.34.3",
    "slugify": "^1.6.6",
    "stripe": "^8.121.0",
    "tldts": "^7.0.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@graphql-tools/schema": "^10.0.24",
    "@graphql-tools/utils": "^10.9.0",
    "@next/eslint-plugin-next": "^15.4.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/bunyan": "^1.8.11",
    "@types/cookie-parser": "^1.4.9",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.3",
    "@types/graphql-resolvers": "^0.3.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/mjml": "^4.7.4",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.15",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "axios": "^1.10.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^15.4.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "next": "^15.4.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsconfig-paths": "^4.2.0"
  }
}
