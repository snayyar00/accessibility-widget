{
  "name": "api",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register server.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "db:create-products": "ts-node -r tsconfig-paths/register scripts/create-products.ts",
    "db:migrate": "knex migrate:latest --knexfile config/knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile config/knexfile.ts",
    "db:create": "knex migrate:make --knexfile config/knexfile.ts",
    "db:down": "knex migrate:down --knexfile config/knexfile.ts",
    "db:seed": "knex seed:run --knexfile config/knexfile.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check \"**/*.{js,ts,tsx,jsx,json,css,scss,md}\"",
    "format:fix": "prettier --write \"**/*.{js,ts,tsx,jsx,json,css,scss,md}\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.817.0",
    "@getbrevo/brevo": "^2.0.0-beta.4",
    "@sentry/node": "^5.30.0",
    "@types/jspdf": "^1.3.3",
    "@types/linkify-it": "^5.0.0",
    "apollo-server-express": "^3.13.0",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.14",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.9.6",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-rate-limit": "^7.5.1",
    "fastest-validator": "^1.8.0",
    "graphql": "^15.5.0",
    "graphql-rate-limit-directive": "^2.0.6",
    "graphql-resolvers": "^0.4.2",
    "graphql-type-json": "^0.3.2",
    "handlebars": "^4.7.6",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "knex": "^0.21.12",
    "linkify-it": "^5.0.0",
    "lodash": "^4.17.20",
    "mjml": "^4.7.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "openai": "^4.27.0",
    "p-limit": "^6.2.0",
    "rate-limiter-flexible": "^7.1.1",
    "sharp": "^0.34.3",
    "slugify": "^1.6.6",
    "stripe": "^8.121.0",
    "tldts": "^7.0.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@graphql-tools/schema": "^10.0.6",
    "@graphql-tools/utils": "^10.1.2",
    "@next/eslint-plugin-next": "^15.4.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/bunyan": "^1.8.7",
    "@types/cookie-parser": "^1.4.2",
    "@types/eslint": "^7.28.0",
    "@types/express": "^4.17.13",
    "@types/graphql-resolvers": "^0.3.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.17.17",
    "@types/mjml": "^4.7.0",
